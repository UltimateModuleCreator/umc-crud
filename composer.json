{
  "name": "umc/module-crud",
  "description": "Magento 2 module for reducing the CRUD boierplate",
  "require": {
    "php": "~7.1.3||~7.2.0||~7.3.0",
    "magento/module-backend": "*",
    "magento/module-config": "*",
    "magento/module-ui": "*",
    "magento/framework": "*"
  },
  "type": "magento2-module",
  "license": [
    "MIT"
  ],
  "scripts": {
    "post-install-cmd": "bin/magento umc:crud:deploy",
    "post-update-cmd": "bin/magento umc:crud:deploy -f"
  },
  "autoload": {
    "files": [
      "registration.php"
    ],
    "psr-4": {
      "Umc\\Crud\\": ""
    }
  }
}
